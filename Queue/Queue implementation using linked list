#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *next;
};
struct node *front=NULL;
struct node *rear=NULL;
int main(){
    int n,value;
    printf("Enter the number of elements to enqueue: ");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        struct node *newNode=malloc(sizeof(struct node));
        scanf("%d",&value);
        newNode->data=value;
        newNode->next=NULL;
        if(rear==NULL){
            front=rear=newNode;
        }else{
            rear->next=newNode;
            rear=newNode;
        }
    }
    printf("\nQueue stored using Linked list:\n");
    struct node *temp=front;
    while(temp!=NULL){
        printf("%d\n",temp->data);
        temp=temp->next;
    }
    return 0;
}
